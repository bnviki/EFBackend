#signup-form
    form(class='form-horizontal')
        .legend
            .label.label-default Sign Up
        .form-group
            label(class="col-md-3 control-label", for="userNameSignup") UserName
            .col-md-7
                input(class="form-control", type="text", id="userNameSignup", placeholder="UserName", ng-model='userSignup.username')
        .form-group
            label(class="col-md-3 control-label", for="inputEmailSignup") Email
            .col-md-7
                input(class="form-control", type="text", id="inputEmailSignup", placeholder="Email", ng-model='userSignup.email')
        .form-group
            label(class="col-md-3 control-label", for="inputPasswordSignup") Password
            .col-md-7
                input(class="form-control", type="password", id="inputPasswordSignup", placeholder="Password", ng-model='userSignup.password')
        .btn.btn-default(type='submit', ng-click='qwerty(userSignup)') Sign up